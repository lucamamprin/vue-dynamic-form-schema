{
  "extends": ["eslint:recommended", "plugin:vue/base"],
  "rules": {
    "semi": ["error", "never"],
    "quotes": ["error", "double"],
    "no-multiple-empty-lines": [2, {"max": 1, "maxEOF": 0}],
    "space-before-function-paren": ["error", "always"],
    "comma-dangle": ["error", {
      "arrays": "always",
      "objects": "always-multiline",
      "functions": "never"
    }],
    "array-bracket-newline": ["error", "always"],
    "indent": [2, 2]
  }
}
